{
  "name": "lalaz/app",
  "description": "Create a new Lalaz application with an interactive wizard",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "lalaz",
    "php",
    "framework",
    "installer",
    "wizard"
  ],
  "authors": [
    {
      "name": "Lalaz Foundation",
      "email": "hi@lalaz.dev"
    }
  ],
  "require": {
    "php": "^8.3",
    "lalaz/framework": "^1.0.0-rc.1"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Lalaz\\Installer\\": "src/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "Lalaz\\Installer\\Installer::run"
    ]
  }
}
